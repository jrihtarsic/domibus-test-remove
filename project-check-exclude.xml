    <suppress>
        <notes><![CDATA[
   file name: activemq-protobuf-1.1.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.activemq\.protobuf/activemq\-protobuf@.*$</packageUrl>
        <!-- we are suppressing all references to activemq-protobuf 1.1 as is the only one used from activemq 1.1 libraries -->
        <cpe>cpe:/a:apache:activemq:1.1</cpe>
    </suppress>
    <suppress>
        <notes><![CDATA[
   file name: commons-collections-3.2.jar
   ]]></notes>
        <gav regex="true">^commons-collections:commons-collections:.*$</gav>
        <!-- Affected users should update the system Apache commons-collections library to at least version 3.2.2.
             In Domibus, we have version 3.2.2-->
        <cve>CVE-2015-6420</cve>
        <!-- In Apache Synapse, by default no authentication is required for Java Remote Method Invocation (RMI).
             To mitigate the issue, we need to limit RMI access to trusted users only.
             In Domibus, RMI is not used. -->
        <cve>CVE-2017-15708</cve>
        <!-- Transitive dependency from OpenCSV - Export CSV feature is only available to logged in users -->
        <vulnerabilityName>Remote code execution</vulnerabilityName>
    </suppress>
    <suppress>
        <notes><![CDATA[
        all netty-*-4.1.34.Final.jar libraries as transitive dependencies to artemis-jms-client:jar:2.10.1
   ]]></notes>
        <!-- we are ignoring all CVEs related to netty* libraries version 4.1.34 as:
         1. jms-artemis-client use 8 of them as transitive dependencies
         2. next version without CVEs is 4.1.44.Final but isn't guaranteed that jms-client will work anymore
         3. to review this decision once we update artemis-jms-client to a newer version
         -->
        <cpe>cpe:/a:netty:netty:4.1.34</cpe>
    </suppress>
    <suppress>
        <notes><![CDATA[
   file name: jackson-databind-2.9.10.2.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson\-databind@.*$</packageUrl>
        <!-- we are not using xbean-reflect libraries -->
        <cve>CVE-2020-8840</cve>
        <!-- we don't use hikari-config library -->
        <cve>CVE-2020-9546</cve>
        <!-- we don't use ibatis-sqlmap library -->
        <cve>CVE-2020-9547</cve>
        <!-- we don't use anteros-core library-->
        <cve>CVE-2020-9548</cve>
        <!-- we are not using apache aries library -->
        <cve>CVE-2020-10672</cve>
        <!-- we are not using caucho quercus library -->
        <cve>CVE-2020-10673</cve>
        <!-- we are not using bus-proxy library -->
        <cve>CVE-2020-10968</cve>
        <!-- we are not using javax.swing -->
        <cve>CVE-2020-10969</cve>
        <!-- not as transitive dependency from any activemq library but from io.dropwizard.metrics -->
        <cve>CVE-2020-11111</cve>
        <!-- we are not using apache commons-proxy -->
        <cve>CVE-2020-11112</cve>
        <!-- we are not using apache open-jpa -->
        <cve>CVE-2020-11113</cve>
    </suppress>
</suppressions>
