<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">
    <suppress>
        <notes><![CDATA[
                This suppresses the checks for any file named: domibus-ext-services-delegate-*.jar
                ]]>
        </notes>
        <gav regex="true">^eu\.domibus:domibus-ext-services-delegate:.*$</gav>
        <cpe>cpe:/a:delegate:delegate</cpe>
    </suppress>
    <suppress>
        <notes><![CDATA[
   file name: geronimo-javamail_1.4_mail-1.8.4.jar
   ]]></notes>
        <gav regex="true">^org\.apache\.geronimo\.javamail:geronimo-javamail_1\.4_mail:.*$</gav>
        <!-- Local users obtain access to unspecified files or directories -->
        <cve>CVE-2008-0732</cve>
        <!-- Hash values for form parameters, we don't have user input on javamail -->
        <cve>CVE-2011-5034</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
   file name: xbean-spring-4.2.jar
   ]]></notes>
        <gav regex="true">^org\.apache\.xbean:xbean-spring:.*$</gav>
        <!-- Local users obtain access to unspecified files or directories -->
        <cve>CVE-2008-0732</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
   file name: activemq-jaas-5.15.6.jar
   ]]></notes>
        <gav regex="true">^org\.apache\.activemq:activemq-jaas:.*$</gav>
        <!-- jolokia API not used -->
        <cve>CVE-2015-5182</cve>
        <!-- Hawtio console not used -->
        <cve>CVE-2015-5183</cve>
        <cve>CVE-2015-5184</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
